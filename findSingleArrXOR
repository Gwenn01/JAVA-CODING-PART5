public class Main {
	public static void main(String[] args) {
		int arr[] = {2, 3, 6, 5, 8, 2, 5, 3, 6};
		System.out.println(findSingle(arr));
	}
	public static int findSingle(int arr[]){
	    int unique = 0;
	    for(int i = 0; i < arr.length; i++){
	        unique = unique ^ arr[i];
	    }
	    
	    return unique;
	}
}