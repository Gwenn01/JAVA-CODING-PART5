import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a Number: ");
		int n = s.nextInt();
		
		System.out.println(isPrime(n));
	}
	public static boolean isPrime(int n){
	   if(n == 1){
	       return false;
	   }
	   if(n == 2 || n == 3 || n == 5){
	       return true;
	   }
	
	    if(n % 2 != 0 && n % 3 != 0 && n % 5 != 0){
	        return true;
	    }
	    
	    return false;
	}
}