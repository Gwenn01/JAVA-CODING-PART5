public class Main {
	public static void main(String[] args) {
		int[] arr = {7, 10, 2, 5, 12, 16, 5, 9};
		
		int max = arr[0];
		
		for(int i = 0; i < arr.length; i++){
		    max = maxValue(max, arr[i]);
		}
		
		System.out.println(max);
	}
	public static int maxValue(int num1, int num2){
	    if(num1 > num2){
	        return num1;
	    }else {
	        return num2;
	    }
	}
}