import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a Number: ");
		int n = s.nextInt();
		
		multiply(n);
	}
	static void multiply(int n){
	    int multiply;
	    for(int i = 1; i <= 10; i++){
	        multiply = n * i;
	        System.out.println(n + " x " + i + " = " + multiply);
	    }
	    
	}
}