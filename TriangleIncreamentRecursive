public class Main {
	public static void main(String[] args) {
		triangle(0, 0);
	}
	static void triangle(int r, int c){
	    if(r == 4){
	        return;
	    }
	    if(r >= c){
	      System.out.print("*");
	      triangle(r, ++c);
	    }else{
	       System.out.println();
	       triangle(++r, 0);
	    }
	    
	}
}