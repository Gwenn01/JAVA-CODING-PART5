public class Main {
	public static void main(String[] args) {
		 int n = 5;
		 int ans = 1, add = 3, temp = 1;
		 for(int i = 1; i <= n; i++){
		     for(int j = 1; j <= i; j++){
		       if(j != 1){
		           ans += 2;
		       }
		       System.out.print(ans + " ");
		     }
		      if(temp == add){
		         temp = 1;
		         add++;
		     }
		     ans += add;
		     temp++;
		     System.out.println();
		 }
		 
	}
}